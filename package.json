{
  "name": "hangman",
  "version": "1.0.0",
  "description": "Word or phrase guessing game",
  "main": "index.js",
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(s?css|less)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "create-tables": "babel-node ./db/dbConnection createAllTables",
    "start": "nodemon ./server/index.js",
    "build": "webpack -d --watch",
    "setup": "npm-run-all -p start create-tables",
    "seed:csv": "node ./db/seed.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debjohnson33/Hangman.git"
  },
  "author": "Deb Johnson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/debjohnson33/Hangman/issues"
  },
  "homepage": "https://github.com/debjohnson33/Hangman#readme",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "make-runnable": "^1.3.8",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "babel-jest": "^26.6.3",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "faker": "^5.1.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "style-loader": "^2.0.0",
    "supertest": "^6.0.1"
  }
}
